{
	"name": "ScrummerTheodo",
	"version": "SCRUMMER_VERSION",
	"description": "Adds Storypoints to Trello",
	"content_scripts": [
		{
      "run_at": "document_end",
			"matches": ["https://trello.com/*"],
			"js": ["scrummer.js"],
			"css": ["scrummer.css"]
		}
	],
	"manifest_version" : 2,
	"icons": {
		"48": "img/icon48.png",
		"128": "img/icon128.png",
		"1024": "img/icon1024.png"
	}
}
